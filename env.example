# 환경 설정
NODE_ENV=production
PORT=3000

# 데이터베이스 설정 (외부 DB 사용 시)
# DB_HOST=your-external-db-host
# DB_PORT=5432
# DB_USERNAME=your-db-user
# DB_PASSWORD=your-db-password
# DB_DATABASE=calmato

# JWT 설정
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRES_IN=7d

# CORS 설정
CORS_ORIGIN=http://localhost:3001,https://yourdomain.com

# Swagger 설정
SWAGGER_ENABLE=true

# 로그 설정
LOG_LEVEL=info 